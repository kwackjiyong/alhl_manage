<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<typeAlias type="com.manage.alhl.dto.UserDTO" alias="UserDTO"></typeAlias>
		<typeAlias type="com.manage.alhl.dto.Shop_ProductDTO" alias="Shop_ProductDTO"></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/UserMapper.xml" />
		<mapper resource="mappers/Shop_Product_Mapper.xml" />
	</mappers>
	
</configuration>
